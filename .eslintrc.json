{
  "root": true,
  "extends": [
    "plugin:jane/recommended",
    "plugin:jane/react",
    "plugin:jane/typescript",
    "plugin:jane/prettier-ts",
    "plugin:jane/jest"
  ],
  "plugins": [
    "jane"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-use-before-define": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
